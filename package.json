{
  "name": "express-prisma-api",
  "version": "1.0.0",
  "description": "API com ExpressJS, TypeScript, Prisma, PostgreSQL e Zod",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
